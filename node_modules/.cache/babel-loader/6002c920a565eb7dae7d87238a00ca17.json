;AAC9C,UAAI,OAAOH,OAAO,CAACG,OAAf,KAA2B,QAA/B,EAAyC;AAAE;AACzC,YAAIC,IAAI,GAAGJ,OAAO,CAACG,OAAnB;AACA,YAAIE,CAAC,GAAGL,OAAO,CAACG,OAAR,GAAkB,IAAIG,IAAJ,EAA1B;AACAD,QAAAA,CAAC,CAACE,OAAF,CAAUF,CAAC,CAACG,OAAF,KAAcJ,IAAxB;AACD;;AACDd,MAAAA,MAAM,IAAI,cAAcU,OAAO,CAACG,OAAR,CAAgBM,WAAhB,EAAxB;AACD,KAlB0B,CAoB3B;;;AACA,QAAI,OAAOT,OAAO,CAACC,IAAf,KAAwB,WAA5B,EAAyC;AAAEX,MAAAA,MAAM,IAAI,WAAWU,OAAO,CAACC,IAA7B;AAAmC,KArBnD,CAsB3B;;;AACA,QAAI,OAAOD,OAAO,CAACU,MAAf,KAA0B,WAA9B,EAA2C;AAAEpB,MAAAA,MAAM,IAAI,aAAaU,OAAO,CAACU,MAA/B;AAAuC,KAvBzD,CAwB3B;;;AACA,QAAIV,OAAO,CAACW,MAAZ,EAAoB;AAAErB,MAAAA,MAAM,IAAI,SAAV;AAAqB;;AAC3CsB,IAAAA,OAAO,CAACC,GAAR,CAAY,WAAZ,EAAwBvB,MAAxB,EA1B2B,CA2B3B;;AACAE,IAAAA,QAAQ,CAACF,MAAT,GAAkBA,MAAlB;AACD,GAhDY;AAiDb;AACAwB,EAAAA,UAlDa,sBAkDFzB,GAlDE,EAkDGW,OAlDH,EAkDY;AACvBA,IAAAA,OAAO,GAAGA,OAAO,IAAI,EAArB;AACAA,IAAAA,OAAO,CAACG,OAAR,GAAkB,CAAC,CAAnB,CAFuB,CAEF;;AACrB,SAAKL,OAAL,CAAaT,GAAb,EAAkB,EAAlB,EAAsBW,OAAtB;AACD;AAtDY,CAAf","sourcesContent":["// key : cookie 名\r\n// value : cookie 值\r\n// options : 可选配置参数\r\n//      options = {\r\n//          expires : 7|new Date(), // 失效时间\r\n//          path : \"/\", // 路径\r\n//          domain : \"\", // 域名\r\n//          secure : true // 安全连接\r\n//      }\r\n\r\nexport default {\r\n  getItem(key) {\r\n    var cookie\r\n    // 获取当前域下所有的 cookie，保存到 cookies 数组中\r\n    var cookies = document.cookie.split('; ')\r\n    // 遍历 cookies 数组中的每个元素\r\n    for (var i = 0, len = cookies.length; i < len; i++) {\r\n      // cookies[i] : 当前遍历到的元素，代表的是 \"key=value\" 意思的字符串，\r\n      // 将字符串以 = 号分割返回的数组中第一个元素表示 key，\r\n      // 第二个元素表示 value\r\n      cookie = cookies[i].split('=')\r\n      // 判断是否是要查找的 key，对查找的 key 、value 都要做解码操作\r\n      if (decodeURIComponent(cookie[0]) === key) {\r\n        return decodeURIComponent(cookie[1])\r\n      }\r\n    }\r\n    // 没有查找到指定的 key 对应的 value 值，则返回 null\r\n    return null\r\n  },\r\n  setItem(key, value, options) {\r\n    var cookie\r\n    /* 存入 设置 */\r\n    // 设置 options 默认为空对象\r\n    options = options || { path: '/' }\r\n    if (!options.path) {\r\n      options.path = '/'\r\n    }\r\n    // key = value，对象 key，value 编码\r\n    cookie = encodeURIComponent(key) + '=' + encodeURIComponent(value)\r\n    // 失效时间\r\n    if ((typeof options.expires) !== 'undefined') { // 有配置失效时间\r\n      if (typeof options.expires === 'number') { // 失效时间为数字\r\n        var days = options.expires\r\n        var t = options.expires = new Date()\r\n        t.setDate(t.getDate() + days)\r\n      }\r\n      cookie += ';expires=' + options.expires.toUTCString()\r\n    }\r\n\r\n    // 路径\r\n    if (typeof options.path !== 'undefined') { cookie += ';path=' + options.path }\r\n    // 域名 兼容测试环境\r\n    if (typeof options.domain !== 'undefined') { cookie += ';domain=' + options.domain }\r\n    // 安全连接\r\n    if (options.secure) { cookie += ';secure' }\r\n    console.log(\"cookie = \",cookie);\r\n    // 保存\r\n    document.cookie = cookie\r\n  },\r\n  // 从所有的 cookie 中删除指定的 cookie\r\n  removeItem(key, options) {\r\n    options = options || {}\r\n    options.expires = -1 // 将失效时间设置为 1 天前\r\n    this.setItem(key, '', options)\r\n  }\r\n}\r\n"]}]}