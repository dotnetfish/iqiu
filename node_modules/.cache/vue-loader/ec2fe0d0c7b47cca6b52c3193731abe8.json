\n    }\r\n\r\n    ::v-deep .el-tabs--left .el-tabs__active-bar.is-left {\r\n      /*height: 0;*/\r\n      top: 10px;\r\n      left: 0;\r\n      right: auto;\r\n      height: 34px!important;\r\n      width: 4px;\r\n\r\n    }\r\n  }\r\n</style>\r\n\r\n\r\n<style lang=\"scss\">\r\n  .fl {\r\n    float: left;\r\n  }\r\n\r\n  .ml5 {\r\n    margin-left: 8px;\r\n  }\r\n\r\n  .mt10 {\r\n    margin-top: 10px;\r\n  }\r\n\r\n  .person-btn {\r\n    margin-top: 60px;\r\n    text-align: center;\r\n\r\n    ::v-deep .el-button {\r\n      width: 152px;\r\n      height: 40px;\r\n      font-size: 15px;\r\n    }\r\n  }\r\n\r\n  .code-down {\r\n    // position: relative;\r\n    .code-show {\r\n      // width: 180px;\r\n      // height: 202px;\r\n      margin-top: -30px;\r\n      text-align: center;\r\n      background-color: #fff;\r\n      border-radius: 10px;\r\n      // box-shadow: 1px 2px 10px 0px #9B9B9B;\r\n      .title {\r\n        font-size: 16px;\r\n        color: #333;\r\n        line-height: 22px;\r\n        font-weight: 500;\r\n      }\r\n\r\n      .info {\r\n        margin-top: 10px;\r\n        font-size: 12px;\r\n        line-height: 17px;\r\n        color: #666;\r\n      }\r\n\r\n      .code-pic {\r\n        width: 120px;\r\n        height: 120px;\r\n        margin: 10px auto;\r\n        border-radius: 4px;\r\n        border: 0.5px solid #ccc;\r\n\r\n        img {\r\n          width: 110px;\r\n          height: 110px;\r\n          margin: 5px;\r\n        }\r\n      }\r\n\r\n      .down-zhushou {\r\n        margin-top: 0px;\r\n        margin-bottom: -20px;\r\n        height: 46px;\r\n        line-height: 46px;\r\n        // border-top: 1px solid #D9D9D9;\r\n        font-size: 14px;\r\n\r\n        /*a {*/\r\n        /*  padding-left: 35px;*/\r\n        /*  background: url('../../assets/icon_kt3.png') no-repeat left center;*/\r\n        /*}*/\r\n      }\r\n    }\r\n  }\r\n</style>\r\n\r\n"]}]}